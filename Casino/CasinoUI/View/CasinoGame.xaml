<Window x:Class="CasinoUI.View.CasinoGame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CasinoUI.View"
        mc:Ignorable="d"
        Title="CasinoGame" Height="450" Width="800">
    <Grid>
        <local:GameCanvas x:Name="MyCanvas" Grid.Column="1" Grid.Row="1">
        </local:GameCanvas>
        <Button x:Name="BtnAddCards" Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="BtnAddCards_Click"/>
        
    </Grid>
</Window>
