<Window x:Class="CasinoUI.View.Poker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CasinoUI.View"
        mc:Ignorable="d"
        Title="Poker" Height="675" Width="1200" FocusManager.FocusedElement="{Binding ElementName=MyCanvas}"
        ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <ImageBrush x:Name="TableBlueBackground"/>
        </Grid.Background>
        <Canvas x:Name="MyCanvas" Focusable="True">
        </Canvas>
    </Grid>
</Window>
