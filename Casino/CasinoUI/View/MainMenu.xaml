<Window x:Class="CasinoUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CasinoUI"
        xmlns:view="clr-namespace:CasinoUI.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1300">
    <Grid>
        <view:GameCanvas x:Name="MyCanvas" Height="800" Width="1200">
        </view:GameCanvas>
        <Button x:Name="BtnAddCards" Content="Button" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="BtnAddCards_Click"/>
    </Grid>
</Window>
